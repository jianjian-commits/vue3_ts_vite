{
  "name": "pc_vue3_project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint:prettier": "prettier --cache --write \"src/**/*.{vue,ts,tsx}\"",
    "lint:eslint": "eslint --cache --fix \"src/**/**.{vue,ts,tsx}\"",
    "lint:style": "stylelint --fix \"src/**/*.{less,css}\"",
    "lint": "pnpm run lint:prettier && pnpm run lint:eslint && pnpm run lint:style",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{vue,ts,tsx}": [
      "pnpm run lint:eslint",
      "pnpm run lint:prettier"
    ],
    "src/**/*.{css,less}": "pnpm run lint:style"
  },
  "dependencies": {
    "axios": "^1.1.3",
    "pinia": "^2.0.23",
    "pinia-plugin-persistedstate": "^2.3.0",
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "@vitejs/plugin-vue": "^3.1.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^9.6.0",
    "husky": "^8.0.1",
    "less": "^4.1.3",
    "lint-staged": "^13.0.3",
    "naive-ui": "^2.33.5",
    "prettier": "^2.7.1",
    "stylelint": "^14.14.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^9.0.0",
    "typescript": "^4.6.4",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-vue-components": "^0.22.8",
    "vite": "^3.1.0",
    "vue-tsc": "^0.40.4"
  }
}